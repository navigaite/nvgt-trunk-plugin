{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/index.{js,ts,tsx}",
    "src/main.{js,ts,tsx}",
    "src/app.{js,ts,tsx}",
    "index.{js,ts,tsx}",
    "main.{js,ts,tsx}",
    "app.{js,ts,tsx}",
    "scripts/**/*.{js,ts}",
    "bin/**/*.{js,ts}"
  ],
  "project": [
    "src/**/*.{js,ts,tsx}",
    "lib/**/*.{js,ts,tsx}",
    "scripts/**/*.{js,ts}",
    "bin/**/*.{js,ts}"
  ],
  "ignore": [
    "dist/**",
    "build/**",
    "coverage/**",
    "node_modules/**",
    "**/*.d.ts",
    "**/*.test.{js,ts,tsx}",
    "**/*.spec.{js,ts,tsx}",
    "**/__tests__/**",
    "**/test/**",
    "**/tests/**"
  ],
  "ignoreBinaries": [
    "rm",
    "cp",
    "mv",
    "mkdir",
    "touch",
    "cat",
    "echo",
    "cd",
    "ls"
  ],
  "ignoreDependencies": [
    "@types/*",
    "typescript",
    "eslint*",
    "prettier*",
    "jest*",
    "vitest*",
    "@testing-library/*",
    "jsdom"
  ],
  "workspaces": {
    ".": {
      "entry": "src/index.{js,ts,tsx}",
      "project": "src/**/*.{js,ts,tsx}"
    }
  },
  "include": ["files", "dependencies", "unlisted", "exports", "nsExports", "classMembers"],
  "exclude": ["enumMembers"],
  "includeEntryExports": true
}
